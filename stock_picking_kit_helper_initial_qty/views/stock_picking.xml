<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form.kit.helper.qty</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock_picking_product_kit_helper.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_kit_helper_ids']/tree/field[@name='product_uom']" position="after">
                <field name="ordered_quantity"/>
            </xpath>
        </field>
    </record>
</odoo>
