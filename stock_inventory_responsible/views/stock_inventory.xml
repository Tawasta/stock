<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id="stock_inventory_resposible_form" model="ir.ui.view">
			<field name="name">stock.inventory.responsible.form</field>
			<field name="model">stock.inventory</field>
			<field name="inherit_id" ref="stock.view_inventory_form"/>
			<field name="arch" type="xml">
				<field name="date" position="after">
					<field name="user_id" options="{ 'no_create': True }"/>
				</field>
			</field>
		</record>

		<record id="stock_inventory_resposible_tree" model="ir.ui.view">
			<field name="name">stock.inventory.responsible.tree</field>
			<field name="model">stock.inventory</field>
			<field name="inherit_id" ref="stock.view_inventory_tree"/>
			<field name="arch" type="xml">
				<field name="date" position="after">
					<field name="user_id"/>
				</field>
			</field>
		</record>		
	</data>
</openerp>