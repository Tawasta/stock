<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="stock_picking_form_view"
            model="ir.ui.view">
            <field name="name">stock.picking.form.week.of.shipment</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id"
                ref="stock.view_picking_form"/>
            <field name="arch"
                type="xml">
                <field name="date_done"
                    position="after">
                    <field name="week_of_shipment"
                        groups="sale.group_sale_order_dates"></field>
                </field>
            </field>
        </record>
    </data>
</odoo>
